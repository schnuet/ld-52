[application]

config/name="Shadow Harvester"
run/main_scene="res://scenes/01_start/SceneStart.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://globals/global.gd"
MenuPopup="*res://globals/MenuPopup.tscn"
MessageBox="*res://globals/MessageBox.tscn"
MusicPlayer="*res://globals/MusicPlayer.tscn"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Globals="*res://addons/godot_helper_pack/core/globals.gd"
SignalMgr="*res://addons/godot_helper_pack/core/signal_mgr.gd"
ServiceMgr="*res://addons/godot_helper_pack/core/service_mgr.gd"
GlobalBlockingColorChangeMonitor="*res://addons/godot_helper_pack/core/2d/shape/global_blocking_color_monitor.tscn"
BackgroundLoader="*res://components/background_loader/BackgroundLoader.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot_helper_pack/plugin.cfg", "res://addons/scene_manager/plugin.cfg" )

[global]

export=false

[godot_helper_pack_plugin]

blocking_global_color=Color( 1, 1, 1, 1 )

[gui]

common/drop_mouse_on_gui_input_disabled=true

